extends layout
block content
  br
  h2 Import CSV file using NodeJS and MongoDB
  br
  p
   a(href='javascript:void(0)' class='btn btn-primary' id='importdata') Import CSV 
   |    
   a(href='javascript:void(0)' class='btn btn-primary' id='fetchdata') Fetch Data
  br  
  p#message.alert.alert-success(style='display:none')
  br
  table.table.table-bordered
    thead
        tr
            th #
            th PantherID
            th FirstName
            th LastName
            th Department
            th Level
            th Campus
            th Degree
            th Email
            th College
            th Year
            th CheckIn
    tbody#trdata
        tr
            td(colspan='3') Click on <strong>Import CSV</strong> 
            button to import and save data to MongoDB. <br />Click on 
            <strong>Fetch Data</strong> button to display data from database
